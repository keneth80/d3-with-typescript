{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-unused-variable": true,
        "no-unused-declaration": true,
        "max-classes-per-file": true,
        "prefer-for-of": false,
        "no-string-literal": false,
        "no-console": false,
        "forin": false,
        "no-bitwise": false,
        "no-empty": false
    },
    "rulesDirectory": []
}