{
    "name": "d3-with-typescript",
    "version": "1.0.0",
    "description": "d3-with-typescript",
    "main": "src/index.ts",
    "scripts": {
        "result": "lite-server -c lite-server-config.json",
        "build": "cross-env NODE_ENV=development webpack --mode development",
        "build-prod": "cross-env NODE_ENV=production webpack --mode production",
        "dev": "webpack-dev-server --open --mode development",
        "bundle-analyze": "webpack --profile --json > stats.json",
        "bundle-analyze-open": "webpack-bundle-analyzer stats.json --port=9900",
        "build:ms": "rmdir /s /q dist | cross-env NODE_ENV=production webpack | npm run mjs",
        "build:linux": "rm -fr dist/* && cross-env NODE_ENV=production webpack && npm run mjs",
        "mjs": "ttsc -p tsconfig.json"
    },
    "sideEffects": false,
    "author": "pretty9967@naver.com",
    "license": "MIT",
    "types": "dist/types/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js"
        },
        "./bar": {
            "import": "./dist/bar.js"
        },
        "./box": {
            "import": "./dist/box.js"
        },
        "./circle": {
            "import": "./dist/circle.js"
        },
        "./line": {
            "import": "./dist/line.js"
        },
        "./mixed": {
            "import": "./dist/mixed.js"
        },
        "./pie": {
            "import": "./dist/pie.js"
        },
        "./topology": {
            "import": "./dist/topology.js"
        }
    },
    "typesVersions": {
        "*": {
            "bar": [
                "dist/types/bar.d.ts"
            ],
            "box": [
                "dist/types/box.d.ts"
            ],
            "circle": [
                "dist/types/circle.d.ts"
            ],
            "line": [
                "dist/types/line.d.ts"
            ],
            "mixed": [
                "dist/types/mixed.d.ts"
            ],
            "pie": [
                "dist/types/pie.d.ts"
            ],
            "topology": [
                "dist/types/topology.d.ts"
            ]
        }
    },
    "files": [
        "dist"
    ],
    "devDependencies": {
        "@babel/core": "^7.12.7",
        "@babel/plugin-proposal-class-properties": "^7.12.1",
        "@babel/plugin-proposal-decorators": "^7.12.1",
        "@babel/plugin-transform-runtime": "^7.12.1",
        "@babel/polyfill": "^7.12.1",
        "@babel/preset-env": "^7.12.7",
        "@types/d3": "^7.4.0",
        "@types/object-hash": "^1.3.3",
        "babel-loader": "^8.2.1",
        "clean-webpack-plugin": "^4.0.0",
        "copy-webpack-plugin": "^11.0.0",
        "cross-env": "^5.2.1",
        "css-loader": "^3.6.0",
        "css-minimizer-webpack-plugin": "^4.2.2",
        "eslint": "^7.14.0",
        "html-webpack-inject-string-plugin": "^1.0.5",
        "html-webpack-plugin": "^5.5.0",
        "style-loader": "^0.23.1",
        "terser-webpack-plugin": "^4.2.3",
        "ts-loader": "^8.0.5",
        "ttypescript": "^1.5.15",
        "typescript": "^4.1.5",
        "webpack": "^5.20.0",
        "webpack-bundle-analyzer": "^4.8.0",
        "webpack-cli": "^5.0.1",
        "webpack-dev-server": "^4.11.1",
        "webpack-merge": "^5.8.0",
        "typescript-transform-paths": "^3.3.1"
    },
    "dependencies": {
        "crossfilter2": "^1.5.4",
        "d3": "^7.8.2",
        "object-hash": "^2.0.3",
        "rxjs": "^6.5.5"
    }
}
